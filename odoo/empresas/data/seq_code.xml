<odoo>
  <data noupdate="1">
    <!-- ESTA es la que usa tu campo: code = seq_emp_code -->
    <record id="seq_emp_code_xmlid" model="ir.sequence">
      <field name="name">Secuencia Código Empresa</field>
      <field name="code">seq_emp_code</field>
      <field name="padding">2</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
      <field name="implementation">standard</field>
    </record>

    <!-- Si también quieres otra secuencia, dale otro id y otro code -->
    <record id="seq_bod_code_xmlid" model="ir.sequence">
      <field name="name">Secuencia Código Bodega</field>
      <field name="code">seq_bod_code</field>
      <field name="padding">4</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
      <field name="implementation">standard</field>
    </record>
  </data>
</odoo>
